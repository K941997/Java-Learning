package PrintTextToFile1;

import java.io.PrintWriter;

/*
 * Print Text To File:
 * */

public class Example {
	public static void main(String[] args) {
		PrintWriter pw = new PrintWriter("D:\\1_K941997_Java\\Apps\\Java_49_IOPrintTextToFile", "UTF-8");
		pw.println("Xin chao, minh la K");
		pw.flush();
	}
}
